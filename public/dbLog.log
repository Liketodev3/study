 SELECT order_id, 
order_user_id, 
op_teacher_id, 
op_lpackage_is_free_trial, 
order_is_paid, 
order_net_amount, 
CONCAT(u.user_first_name, " " , u.user_last_name) AS learner_username, 
CONCAT(t.user_first_name, " " , t.user_last_name) AS teacher_username, 
COALESCE(NULLIF(sl.tlanguage_name, ""), tlang.tlanguage_identifier) AS language, 
order_currency_code
 FROM 
`tbl_orders` o  
INNER JOIN `tbl_order_products`  AS op  
 ON o.order_id = op.op_order_id  
INNER JOIN `tbl_users`  AS u  
 ON o.order_user_id = u.user_id  
INNER JOIN `tbl_users`  AS t  
 ON op.op_teacher_id = t.user_id  
INNER JOIN `tbl_scheduled_lesson_details`  AS sld  
 ON o.order_id = sld.sldetail_order_id  
INNER JOIN `tbl_scheduled_lessons`  AS slns  
 ON sld.sldetail_slesson_id = slns.slesson_id  
INNER JOIN `tbl_teaching_languages`  AS tlang  
 ON slns.slesson_slanguage_id = tlang.tlanguage_id  
LEFT OUTER JOIN `tbl_teaching_languages_lang`  AS sl  
 ON sl.tlanguagelang_tlanguage_id = tlang.tlanguage_id AND tlanguagelang_lang_id = 1  
GROUP BY order_id 
ORDER BY order_date_added desc 
limit 0, 10